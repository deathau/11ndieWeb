---
layout: base
---
<article class="h-entry">
  {% include partials/h-card.html %}
  {% assign author = site.author %}
  {% assign url = page.url %}
  {% include partials/authortime.liquid %}

  <div class="p-name e-content">
    {{ author_name }}
    {{ content | safe }}

    {% for url in photo %}
      <img src="{{ url }}" alt="image">
    {% endfor %}

  </div>
  {% include partials/webmentions %}
</article>
